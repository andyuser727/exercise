<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>

        .paddingImage {

            padding-left: 130px;
            padding-top: 25px;
        }
    </ui:style>
    <g:DockLayoutPanel unit='EM'>
        <g:north size='6.5'>
            <g:FlowPanel ui:field="headerPanel">
                <g:FlowPanel addStyleNames='{style.paddingImage}'>
                    <g:Image ui:field="addressLogo" />
                </g:FlowPanel>
            </g:FlowPanel>

        </g:north>

        <g:lineStart size='10'>
            <g:ScrollPanel>
                <g:SimplePanel />
            </g:ScrollPanel>
        </g:lineStart>

        <g:center>
            <g:DockLayoutPanel ui:field='mainDockPanel' unit='EM'>
                 <g:center>
                    <g:FlowPanel>
                        <g:SimplePanel ui:field="contentWrapperPanel" />
                    </g:FlowPanel>
                </g:center>
            </g:DockLayoutPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>

